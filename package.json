{
  "name": "di-ipv-core-front",
  "version": "0.0.1",
  "description": "User interface for the Identity Proofing and Verification (IPV) system within the GDS digital identity platform, GOV.UK Sign In.",
  "main": "src/app.ts",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "node --enable-source-maps build/app.js",
    "start-dev": "ts-node src/app.ts | pino-pretty",
    "dev": "nodemon --exec \"node --inspect=0.0.0.0:9228 --require ts-node/register src/app.ts\" --ext js,ts,json",
    "build": "npm run build-js && npm run build-sass && npm run copy-images",
    "build-js": "mkdir -p dist/public/javascripts; uglifyjs node_modules/@govuk-one-login/frontend-analytics/lib/analytics.js assets/javascript/**/**/*.js assets/javascript/**/*.js -o dist/public/javascripts/application.js -c -m && cp node_modules/govuk-frontend/dist/govuk/govuk-frontend.min.js dist/public/javascripts/ && cp node_modules/@govuk-one-login/spinner/spinner-app.js dist/public/javascripts/",
    "build-sass": "rm -rf dist/public/style.css && sass --no-source-map assets/scss/application.scss dist/public/stylesheets/application.css --style compressed",
    "build-service-unavailable": "mkdir -p dist/public/html && ts-node ./build-scripts/build-service-unavailable-html.ts dist/public/html/service-unavailable.html",
    "watch-sass": "sass --no-source-map assets/scss/application.scss dist/public/stylesheets/application.css --style compressed --watch",
    "copy-images": "copyfiles -u 2 assets/images/** dist/public/images",
    "lint": "prettier --check src && eslint src/",
    "lint-fix": "prettier --write src && eslint --fix src/",
    "test": "mocha",
    "test:coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "test:watch": "mocha --watch",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphagov/di-ipv-core-front.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alphagov/di-ipv-core-front/issues"
  },
  "homepage": "https://github.com/alphagov/di-ipv-core-front#readme",
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@types/cfenv": "1.2.4",
    "@types/chai": "4.3.20",
    "@types/chai-as-promised": "7.1.8",
    "@types/cookie-parser": "1.4.8",
    "@types/express": "5.0.0",
    "@types/express-session": "1.18.1",
    "@types/mocha": "10.0.10",
    "@types/nunjucks": "3.2.6",
    "@types/overload-protection": "1.2.4",
    "@types/proxyquire": "1.3.31",
    "@types/qrcode": "1.5.5",
    "@types/sinon": "17.0.3",
    "@types/sinon-chai": "3.2.12",
    "@types/ua-parser-js": "0.7.39",
    "@types/uid-safe": "2.1.5",
    "chai": "4.5.0",
    "chai-as-promised": "7.1.2",
    "copy-webpack-plugin": "13.0.0",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "mocha": "10.8.2",
    "nodemon": "3.1.7",
    "nyc": "17.1.0",
    "pino-pretty": "10.3.1",
    "prettier": "3.5.0",
    "proxyquire": "2.1.3",
    "sass": "1.86.0",
    "sinon": "19.0.2",
    "sinon-chai": "3.7.0",
    "terser-webpack-plugin": "5.3.10",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.27.0",
    "uglify-js": "latest",
    "webpack": "5.98.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.774.0",
    "@aws-sdk/client-ssm": "3.759.0",
    "@govuk-one-login/data-vocab": "1.9.2",
    "@govuk-one-login/frontend-analytics": "2.0.1",
    "@govuk-one-login/frontend-language-toggle": "2.2.1",
    "@govuk-one-login/frontend-passthrough-headers": "1.3.0",
    "@govuk-one-login/frontend-vital-signs": "0.1.2",
    "@govuk-one-login/spinner": "1.1.0",
    "axios": "1.8.2",
    "body-parser": "2.1.0",
    "cfenv": "1.2.4",
    "connect-dynamodb": "3.0.5",
    "cookie-parser": "1.4.7",
    "copyfiles": "2.4.1",
    "csrf-sync": "4.0.3",
    "dotenv": "16.4.5",
    "express": "5.0.1",
    "express-session": "1.18.1",
    "govuk-frontend": "5.7.0",
    "i18next": "23.16.8",
    "i18next-fs-backend": "2.6.0",
    "i18next-http-middleware": "3.7.0",
    "nunjucks": "3.2.4",
    "overload-protection": "1.2.3",
    "pino": "8.21.0",
    "pino-http": "9.0.0",
    "qrcode": "1.5.4",
    "sanitize-filename": "1.6.3",
    "ua-parser-js": "2.0.0"
  }
}
